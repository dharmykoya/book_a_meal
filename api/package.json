{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "Book-A-Meal is an application that connect customers and food vendors together.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "test": "mocha **/*.spec.js --require @babel/register --timeout 30000",
    "test:travis": "mocha **/*.spec.js --require @babel/register --exit",
    "test-watch": "nodemon --exec \"npm test\"",
    "babel-build": "babel src -d dist",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run babel-build",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm run test:travis && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage",
    "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*",
    "migrate": "npm run sequelize db:migrate",
    "migrate-test": "NODE_ENV=test npm run sequelize db:migrate",
    "seed": "npm run sequelize db:seed:all",
    "seed-test": "NODE_ENV=test npm run sequelize db:seed:all"
  },
  "heroku-run-build-script": true,
  "author": "Damilola Adekoya",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "pg": "^7.8.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dharmykoya/book_a_meal.git"
  },
  "keywords": [
    "Book-a-Meal",
    "Andela",
    "JWT",
    "Sequelize",
    "PostgreSQL",
    "Authentication",
    "Single Repository Pattern"
  ],
  "bugs": {
    "url": "https://github.com/dharmykoya/book_a_meal/issues"
  },
  "homepage": "https://github.com/dharmykoya/book_a_meal#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "expect": "^1.20.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "pg": "^7.8.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "supertest": "^3.4.2"
  }
}
